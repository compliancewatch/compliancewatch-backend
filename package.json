{
  "name": "compliancewatch-backend",
  "version": "1.0.0",
  "main": "railway-start.js",
  "type": "module",
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "start": "node railway-start.js",
    "scrape": "node -e \"import('dotenv/config').then(() => import('./scrapers/fatf.js'))\"",
    "test": "node scripts/test-system.js",
    "build": "npm install --omit=dev && npm run scrape",
    "healthcheck": "node -e \"import('./services/database.js').then(db => db.testConnection())\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.8",
    "axios": "^1.6.2",
    "dotenv": "^17.0.0",
    "express": "^4.18.2",
    "node-cron": "^3.0.2",
    "puppeteer": "^19.11.1",
    "telegraf": "^4.12.2",
    "winston": "^3.11.0",
    "npm-force-resolutions": "^0.0.10"
  },
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "resolutions": {
    "puppeteer": "19.11.1",
    "playwright-core": "1.40.0"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}